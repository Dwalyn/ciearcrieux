{# templates/components/ProductSearch.html.twig #}
{# for the Live Component to work, there must be a single root element
   (e.g. a <div>) where the attributes are applied to #}
<div {{ attributes }}>
    <input
            type="search"
            data-model="lastname"
    >
    <ul>
        {% for user in this.users %}
            <li>{{ user }}</li>
        {% endfor %}
    </ul>
</div>
